<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.reedcwilson.synclds</string>
	<key>ProgramArguments</key>
  <array>
    <string>#HOME/.pyenv/versions/2.7.9/bin/python</string>
    <string>#PROJECT/update.py</string>
  </array>
	<key>RunAtLoad</key>
	<true/>
  <key>StandardOutPath</key>
  <string>#PROJECT/out.log</string>
  <key>StandardErrorPath</key>
  <string>#PROJECT/out.log</string>
  <key>StartCalendarInterval</key>
  <dict>
    <key>Weekday</key>
    <integer>5</integer>
    <key>Hour</key>
    <integer>9</integer>
    <key>Minute</key>
    <integer>30</integer>
  </dict>
</dict>
</plist>
